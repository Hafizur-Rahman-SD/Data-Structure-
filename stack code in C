#include<stdio.h> //heder file.
#define CAPACITY 3
int stack [CAPACITY];
int top= -1;

void push(int x){
if (top < CAPACITY -1){
top= top+1;
stack [top]=x;

printf("Sucessfully added item: %d\n",x);
}
else {
printf("Exception No space \n");}
}

int pop(){
if(top >=0){
int val =stack [top];
top = top -1;
return val;
}
printf(" Execption from pop! Empty stack\n");
return -1;
}
